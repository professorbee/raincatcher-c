  RESCOMP = windres
  TARGETDIR = ../bin/Debug
  TARGET = $(TARGETDIR)/raincatcher-c
  OBJDIR = obj/x64/Debug
  DEFINES += -DDEBUG -DPLATFORM_DESKTOP -DGRAPHICS_API_OPENGL_33
  INCLUDES += -I. -I../raylib/src
